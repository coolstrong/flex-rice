{
    "popupCloseDelay": 700,
    "transitionDuration": 150,
    "systray": {
        "ignore": ["ulauncher"]
    },
    "workspace": {
        "ignore": [
            "wofi",
            "kitty-dropterm",
            "org.kde.polkit-kde-authentication-agent-1",
            "ulauncher"
        ],
        "perMonitor": 4
    },
    "keyboard": {
        "default": {
            "name": "compx-2.4g-wireless-receiver-keyboard",
            "keymap": "en"
        }
    },
    "time": {
        "locale": "lt",
        "timezones": ["Asia/Dubai", "Europe/Moscow", "Europe/Berlin"]
    }
}
